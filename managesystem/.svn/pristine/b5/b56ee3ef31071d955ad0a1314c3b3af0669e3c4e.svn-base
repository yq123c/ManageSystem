package com.five.managesystem.login.entity;

import org.hibernate.validator.constraints.Length;
import org.hibernate.validator.constraints.NotBlank;

public class LoginVo {
	
	@Length(min=2,max=20,message="{UserVo.username.length}")
	@NotBlank(message="{UserVo.username}")
	private String username;
	
	@Length(min=5,max=20,message="{UserVo.password.length}")
	@NotBlank(message="{UserVo.password}")
	private String password;
	
	public String getUsername() {
		return username;
	}
	public void setUsername(String username) {
		this.username = username;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
}
