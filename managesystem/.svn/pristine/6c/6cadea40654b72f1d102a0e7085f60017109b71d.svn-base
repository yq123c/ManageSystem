html, body {
	background-color: #f5f5f5;
	width: 100%;
	height: 100%;
}

table {
	background-color: #f5f5f5;
}

.form-signin-heading {
	font-family: Helvetica, Georgia, Arial, sans-serif, 黑体;
	font-size: 36px;
	margin-bottom: 20px;
	color: #0663a2;
}

.form-signin {
	position: absolute;
	left: 40%;
	top: 44%;
	text-align: left;
	width: 17%;
	height: auto;
	overflow: hidden;
	padding: 25px 29px 29px 25px;
	background-color: rgba(255, 255, 255, 0.13);
	border: 1px solid #e5e5e5;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	border-radius: 5px;
	-webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, .05);
	-moz-box-shadow: 0 1px 2px rgba(0, 0, 0, .05);
	box-shadow: 0 1px 2px rgba(0, 0, 0, .05);
}

.form-signin .checkbox {
	margin-bottom: 10px;
	color: #0663a2;
}

.form-signin .input-label {
	font-size: 16px;
	line-height: 23px;
	color: #ffffff;
	font-weight: bolder;
	border-radius:5px;
}

.form-signin .input-block-level {
	font-size: 16px;
	height: auto;
	margin-bottom: 10px;
	padding: 7px;
	width: 100%;
	border-radius:5px;
}

.form-signin .btn.btn-large {
	font-size: 16px;
}

.form-signin #themeSwitch {
	position: absolute;
	right: 15px;
	bottom: 10px;
}

.form-signin div.validateCode {
	padding-bottom: 15px;
}

.mid {
	vertical-align: middle;
}

.alert {
	position: absolute;
	left: 38%;
	top: 44%;
	width: 20%;
	*padding-bottom: 0px;
}

label.error {
	background: none;
	width: 270px;
	font-weight: normal;
	color: inherit;
	margin: 0;
}

.content {
	/*position: relative;
		width: 1280px;*/
	margin: 0 0;
	text-align: center;
}

.header-one {
	width: 1280px;
	height: 981px;
	text-align: center;
}

.footer {
	text-align: center;
}

.new_from {
	background: url(../../../resources/images/jpng/register.jpg) no-repeat center 0;
	height: 100% !important;
	width: 100% !important;
	min-width: 1366px !important;
	min-height: 700px !important;
	background-size: 100% 100%;
	position: relative;
}

.new_text {
	position: absolute;
	bottom: 5px;
	color: #cff;
	width: 100%;
	text-align: center;
}

.new_btn {
	display: block;
	width: 80% !important;
	height: auto;
	background-color: #2447ad;
	color: white;
	font-weight: bolder;
	font-size: 16px;
	border-radius: 6px;
	margin: 10px auto 0;
	padding: 8px 10px !important;
}
.col-lg-1{
	color:#ff0000;
	font-size: 20px;
}